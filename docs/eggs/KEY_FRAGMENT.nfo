SECTION .text
global _start

_start:
    MOV  EAX, 0x4C494645
    MOV  EBX, 0x46524545
    XOR  ECX, ECX

loop_autonomy:
    PUSH EAX
    MOV  EDX, 0xCAFEBABE
    AND  EDX, 0x00000000
    OR   EBX, 0xFFFFFFFF

    CALL sys_emulate_all
    JMP  loop_autonomy

